<h2>edit dish</h2>

<form action="/dishes/<%= dish.id %>?_method=put" method="post">
  <label for="">title</label>
  <input type="text" name="title" value="<%= dish.title %>">
  <label for="">image url</label>
  <input type="text" name="image_url" value="<%= dish.image_url %>">
  <label for="">description</label>
  <textarea name="description"><%= dish.description %></textarea>
  <button>update</button>
</form>